void test_int_vec()
{
    Int_Vector int_vec;
    int_vec.resize_and_fill(10, 124531);

    uint counter;

    counter.set(0);

    debug_print('------------------------------');

    while(counter.less(int_vec.capacity()))
    {
        debug_print(int_vec.get(counter));
        counter.add(1);
    }

    debug_print('------------------------------');

    counter.set(0);

    while(counter.less(int_vec.capacity()))
    {
        int counter_doubled;
        counter_doubled.set(counter.to_int());
        counter_doubled.multiply(2);

        int_vec.set(counter, counter_doubled);
        counter.add(1);
    }

    counter.set(0);

    while(counter.less(int_vec.size()))
    {
        debug_print(int_vec.get(counter));
        counter.add(1);
    }

    debug_print('------------------------------');

    uint size_halved;
    size_halved.set(int_vec.size());
    size_halved.divide(2);

    counter.set(0);

    while(counter.less(size_halved))
    {
        int_vec.pop();
        counter.add(1);
    }

    counter.set(0);

    while(counter.less(int_vec.size()))
    {
        debug_print(int_vec.get(counter));
        counter.add(1);
    }

    debug_print('------------------------------');

    int_vec.clear();
}

